{
  "name": "influx-explorer",
  "description": "InfuxDB query editor and administrator UI",
  "author": "Jan Grzegorowski",
  "version": "0.3.1",
  "private": false,
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^3.1.0",
    "@material-ui/icons": "^3.0.1",
    "apollo-cache-inmemory": "^1.2.7",
    "apollo-client": "^2.4.2",
    "apollo-link": "^1.2.3",
    "apollo-link-state": "^0.4.2",
    "axios": "^0.18.0",
    "date-fns": "^2.0.0-alpha.16",
    "final-form": "^4.9.1",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "history": "^4.7.2",
    "influx-api": "^0.1.4",
    "lodash": "^4.17.10",
    "material-ui-password-field": "^2.0.1",
    "papaparse": "^4.6.0",
    "qs": "^6.5.2",
    "react": "^16.4.1",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.4.1",
    "react-final-form": "^3.6.5",
    "react-inspector": "^2.3.0",
    "react-popper": "^1.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.0.4"
  },
  "scripts": {
    "clean": "rm -r dist/",
    "start": "./update-env.sh && react-scripts start",
    "build": "./update-env.sh && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "lint": "yarn eslint src",
    "eject": "react-scripts eject",
    "start:electron": "./update-env.sh && APP_ELECTRON=true react-scripts start",
    "build:electron": "./update-env.sh && APP_ELECTRON=true react-scripts build",
    "electron:dev": "ELECTRON_START_URL=http://localhost:3000 electron .",
    "electron": "electron .",
    "electron:stag": "yarn clean && electron-builder -m -c.compression=store -c.extraMetadata.main=build/electron.js",
    "electron:prod": "yarn clean && yarn electron:mac",
    "electron:mac": "yarn clean && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:win": "yarn clean && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:lin": "yarn clean && electron-builder -l -c.extraMetadata.main=build/electron.js"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "benchmark": "^2.1.4",
    "electron": "^3.0.3",
    "electron-builder": "^20.27.1",
    "electron-debug": "^2.0.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-react-app": "^3.0.3",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jest": "^21.24.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.82.0",
    "flow-typed": "^2.5.1"
  },
  "main": "public/electron.js",
  "homepage": ".",
  "build": {
    "appId": "com.grzegorowski.influx-explorer",
    "productName": "Influx Explorer",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "target": "dmg",
      "category": "utilities",
      "type": "development"
    },
    "win": {
      "target": "nsis",
      "icon": "./assets/icon.png"
    },
    "linux": {
      "target": "deb",
      "category": "Development"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
